<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.ModelHouseholdParametersFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <RelativeLayout
            android:id="@+id/health_row"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/list_item_background_selctor"
            android:padding="20dp">

            <ImageView
                android:id="@+id/health_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:src="@drawable/ic_action_health" />

            <ImageView
                android:id="@+id/land_arrow_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/row_goto" />

            <org.smartregister.view.customcontrols.CustomFontTextView
                android:id="@+id/textview_land"
                style="@style/CustomFontTextViewStyle.ListView.Normal.MultipleLines"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/health_image"
                android:gravity="center_horizontal"
                android:text="Afya"
                android:textColor="@android:color/black" />

            <org.smartregister.view.customcontrols.CustomFontTextView
                android:id="@+id/text_view_land_profile"
                style="@style/CustomFontTextViewStyle.ListView.Normal.MultipleLines"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textview_land"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/health_image"
                android:text="Completion: 80"
                android:textColor="@android:color/darker_gray" />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/light_grey" />
        <RelativeLayout
            android:id="@+id/social_integration_row"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/list_item_background_selctor"
            android:padding="20dp">

            <ImageView
                android:id="@+id/social_integration_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:src="@drawable/ic_action_social_integration" />

            <ImageView
                android:id="@+id/social_integration_arrow_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/row_goto" />

            <org.smartregister.view.customcontrols.CustomFontTextView
                android:id="@+id/textview_social_integration"
                style="@style/CustomFontTextViewStyle.ListView.Normal.MultipleLines"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/social_integration_image"
                android:gravity="center_horizontal"
                android:text="Utengamano"
                android:textColor="@android:color/black" />

            <org.smartregister.view.customcontrols.CustomFontTextView
                android:id="@+id/text_view_social_integration_profile"
                style="@style/CustomFontTextViewStyle.ListView.Normal.MultipleLines"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textview_social_integration"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/social_integration_image"
                android:text="Completion: 30"
                android:textColor="@android:color/darker_gray" />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/light_grey" />
        <RelativeLayout
            android:id="@+id/land_row"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/list_item_background_selctor"
            android:padding="20dp">

            <ImageView
                android:id="@+id/land_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:src="@drawable/ic_action_land" />

            <ImageView
                android:id="@+id/health_arrow_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/row_goto" />

            <org.smartregister.view.customcontrols.CustomFontTextView
                android:id="@+id/textview_health"
                style="@style/CustomFontTextViewStyle.ListView.Normal.MultipleLines"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/land_image"
                android:gravity="center_horizontal"
                android:text="Ardhi"
                android:textColor="@android:color/black" />

            <org.smartregister.view.customcontrols.CustomFontTextView
                android:id="@+id/text_view_health_profile"
                style="@style/CustomFontTextViewStyle.ListView.Normal.MultipleLines"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textview_health"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/land_image"
                android:text="Completion: 20"
                android:textColor="@android:color/darker_gray" />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/light_grey" />
        <RelativeLayout
            android:id="@+id/agriculture_row"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/list_item_background_selctor"
            android:padding="20dp">

            <ImageView
                android:id="@+id/agriculture_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:src="@drawable/ic_action_agriculture" />

            <ImageView
                android:id="@+id/agriculture_arrow_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/row_goto" />

            <org.smartregister.view.customcontrols.CustomFontTextView
                android:id="@+id/textview_agriculture"
                style="@style/CustomFontTextViewStyle.ListView.Normal.MultipleLines"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/agriculture_image"
                android:gravity="center_horizontal"
                android:text="Kilimo"
                android:textColor="@android:color/black" />

            <org.smartregister.view.customcontrols.CustomFontTextView
                android:id="@+id/text_view_agriculture_profile"
                style="@style/CustomFontTextViewStyle.ListView.Normal.MultipleLines"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textview_agriculture"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/agriculture_image"
                android:text="Completion: 0"
                android:textColor="@android:color/darker_gray" />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/light_grey" />
        <RelativeLayout
            android:id="@+id/animal_husbandry_row"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/list_item_background_selctor"
            android:padding="20dp">

            <ImageView
                android:id="@+id/animal_husbandry_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:src="@drawable/ic_action_animals" />

            <ImageView
                android:id="@+id/animal_husbandry_arrow_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/row_goto" />

            <org.smartregister.view.customcontrols.CustomFontTextView
                android:id="@+id/textview_animal_husbandry"
                style="@style/CustomFontTextViewStyle.ListView.Normal.MultipleLines"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/animal_husbandry_image"
                android:gravity="center_horizontal"
                android:text="Ufugaji"
                android:textColor="@android:color/black" />

            <org.smartregister.view.customcontrols.CustomFontTextView
                android:id="@+id/text_view_animal_husbandry_profile"
                style="@style/CustomFontTextViewStyle.ListView.Normal.MultipleLines"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textview_animal_husbandry"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/animal_husbandry_image"
                android:text="Completion: 0"
                android:textColor="@android:color/darker_gray" />
        </RelativeLayout>
    </LinearLayout>

</FrameLayout>